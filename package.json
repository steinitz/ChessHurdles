  "main": "index.js",
  "type": "module",
  "config": {
    "playwright": "dotenv -e .env.test -- playwright test --headed --config=stzUser/test/e2e/config/playwright.config.ts"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && echo '\nTo start the production server, use: pnpm start:prod'",
    "start:prod": "node bootstrap.env.mjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "$npm_package_config_playwright",
    "test:e2e:contact": "$npm_package_config_playwright -- contact-flow.spec.ts",
    "test:e2e:signup": "$npm_package_config_playwright -- signup-flow.spec.ts",
    "test:e2e:change-email": "$npm_package_config_playwright -- change-email-flow.spec.ts",
    "test:e2e:password-reset": "$npm_package_config_playwright -- password-reset-flow.spec.ts",
    "test:e2e:password-change": "$npm_package_config_playwright -- password-change.spec.ts",
    "test:e2e:mailpit": "$npm_package_config_playwright -- mailpit.spec.ts",
    "test:e2e:create-verified-user": "$npm_package_config_playwright -- createVerifiedTestUser.spec.ts",
    "test:e2e:ui": "$npm_package_config_playwright --ui",
    "test:all": "vitest run && $npm_package_config_playwright",
    "typecheck": "pnpm --package=typescript dlx tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
